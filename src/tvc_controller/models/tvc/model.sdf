    <joint name="servo0_roll_joint" type="revolute">
      <parent>gimbal_mount</parent>
      <child>servo0_roll</child>
      <!-- <pose>0 0 0.1675 0 0 0</pose> -->
      <axis>
        <xyz>
        1 0 0
        </xyz>
        <limit>
          <lower>-1</lower>
          <upper>1</upper>
          <!-- <effort>1.0</effort>
          <velocity>10.0</velocity> -->
        </limit>
        <!-- <dynamics>
          <damping>0.09</damping>
          <friction>0.001</friction>
        </dynamics> -->
      </axis>
    </joint>

    <joint name="servo1_pitch_joint" type="revolute">
      <parent>servo0_roll</parent>
      <child>servo1_pitch</child>
      <!-- <pose>0 0 0.1675 0 0 0</pose> -->
      <axis>
        <xyz>
        0 -1 0
        </xyz>
        <limit>
          <lower>-1</lower>
          <upper>1</upper>
          <!-- <effort>1.0</effort>
